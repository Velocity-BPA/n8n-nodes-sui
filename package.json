{
  "name": "n8n-nodes-sui",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Sui blockchain providing 13 resources and 80+ operations for transactions, objects, NFTs, staking, DeFi, and smart contracts. Includes real-time event triggers.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-sui.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-sui",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "sui",
    "blockchain",
    "web3",
    "defi",
    "nft",
    "move",
    "crypto",
    "mysten",
    "staking",
    "automation"
  ],
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/nodes/Sui/Sui.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SuiNetwork.credentials.js",
      "dist/credentials/SuiScan.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Sui/Sui.node.js",
      "dist/nodes/Sui/SuiTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.23.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@mysten/bcs": "^1.2.0",
    "@mysten/sui": "^1.17.0",
    "axios": "^1.6.0",
    "ws": "^8.16.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
